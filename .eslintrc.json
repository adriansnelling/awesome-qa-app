{
    "extends": [ "plugin:react/recommended", "airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "prettier/prettier": ["error", { "endOfLine": "auto" }],
      "no-else-return": "off",
      "react/prop-types": 0
    }
  }
